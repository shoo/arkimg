{
	"authors": [
		"SHOO"
	],
	"copyright": "Copyright Â© 2024, SHOO",
	"description": "Encrypted image library",
	"license": "BSL-1.0",
	"name": "arkimg",
	"dependencies": {
		"jpeg-turbo": {"version": "~>1.3.0", "optional": true},
		"libwebp": {"version": "~>0.4.0", "optional": true},
		"libpng": "~>1.6.0",
		"openssl-static": "~>1.0.5+3.0.8"
	},
	"targetType": "library",
	"importPaths": ["src"],
	"sourcePaths": ["src"],
	"libs-windows-dmd": ["User32"],
	"lflags-windows-x86": ["/LIBPATH:$PACKAGE_DIR/lib/windows-x86", "/LIBPATH:$OPENSSL_STATIC_PACKAGE_DIR"],
	"lflags-windows-x86_64": ["/LIBPATH:$PACKAGE_DIR/lib/windows-x86_64", "/LIBPATH:$OPENSSL_STATIC_PACKAGE_DIR"],
	"lflags-posix-x86": ["-lcrypto", "-lpng16"],
	"buildTypes": {
		"vscode-debug": { "buildOptions": ["debugMode", "debugInfoC", "unittests"] },
		"vscode-debug-cov": { "buildOptions": ["debugMode", "coverage", "debugInfoC", "unittests"] }
	},
	"subPackages": [
		"examples/arkimg_cli"
	]
}
